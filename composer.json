{
    "name": "edlugz/vas",
    "description": "This package provides you with a simple tool to make requests to VAS SMS API",
    "license": "MIT",
    "authors": [
        {
            "name": "Eddy Lugaye",
            "email": "eddy.lugaye@gmail.com"
        }
    ],
    "homepage": "https://github.com/edlugz/vas",
    "keywords": ["Laravel", "VAS"],
    "require": {
       "illuminate/support": "^10.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "EdLugz\\VAS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdLugz\\VAS\\VASServiceProvider"
            ],
            "aliases": {
                "VAS": "EdLugz\\VAS\\Facades\\VAS"
            }
        }
    }
}
